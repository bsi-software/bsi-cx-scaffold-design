<div data-bsi-element="telefon" style="padding-bottom: 15px;">
    <div data-bsi-element-part="form-field"
        class="reverse-for-required text-field"
        data-bsi-form-field-fixed-type=""
        id="tel-wrapper">
        <input id="telefon"
            name="telefon"
            type="tel"
            class="custom-form-field"
            placeholder="{{ properties.translation.labels.tel }}"
            spellcheck="true"
            required="" />
        <div class="error-message isVisible">
            {{ properties.translation.phone_error }}
        </div>
        <label class="text-field-label" for="telefon">
            {{ properties.translation.labels.tel }}
        </label>
    </div>
</div>
