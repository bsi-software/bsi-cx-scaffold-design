<div data-bsi-element="name-last" style="padding-bottom: 15px;">
    <div data-bsi-element-part="form-field"
        class="reverse-for-required text-field"
        data-bsi-form-field-fixed-type=""
        id="last-name-wrapper">
        <input id="last-name"
            name="last-name"
            type="text"
            class="custom-form-field"
            spellcheck="true"
            placeholder="{{ properties.translation.labels.last_name }}"
            required="" />
        <label class="text-field-label" for="last-name">
            {{ properties.translation.labels.last_name }}
        </label>
    </div>
</div>
